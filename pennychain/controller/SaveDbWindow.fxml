                        <Insets left="20.0" />
                     </VBox.margin>
                  </ListView>
               </children>
            </VBox>
            <VBox>
               <children>
                  <Button fx:id="saveExisting" minWidth="150.0" mnemonicParsing="false" onMouseClicked="#handleSaveExisting" text="Save Selected">
                     <VBox.margin>
                        <Insets left="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="saveNew" layoutX="20.0" layoutY="10.0" minWidth="150.0" mnemonicParsing="false" onMouseClicked="#handleSaveNew" text="Add New Project">
                     <VBox.margin>
                        <Insets left="10.0" top="5.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="cancel" layoutX="20.0" layoutY="40.0" minWidth="150.0" mnemonicParsing="false" onMouseClicked="#handleCancel" text="Cancel">
                     <VBox.margin>
                        <Insets left="10.0" top="5.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </children>
      </HBox>
   </center>
   <top>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <Label alignment="CENTER" text="Save to Online Database">
